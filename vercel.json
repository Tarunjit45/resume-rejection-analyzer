{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/index.js"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/public/$1"
        }
    ],
    "functions": {
        "backend/index.js": {
            "runtime": "@vercel/node"
        }
    }
}
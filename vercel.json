{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/frontend/$1"
        }
    ]
}